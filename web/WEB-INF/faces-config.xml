<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>Administrador/*</from-view-id>
        <navigation-case>
            <from-outcome>caso3</from-outcome>
            <to-view-id>/Administrador/adminP2.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>caso4</from-outcome>
            <to-view-id>/Administrador/adminP3.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>caso1</from-outcome>
            <to-view-id>/Administrador/fAdmin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Padmin</from-outcome>
            <to-view-id>/Administrador/pAdminCRUD.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>caso5</from-outcome>
            <to-view-id>/Administrador/AdminNotas.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
